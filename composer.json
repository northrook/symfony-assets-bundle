{
  "name": "northrook/symfony-assets-bundle",
  "description": "Simple assets management for Symfony.",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\Assets\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/deprecation-contracts": "^3.0",
    "northrook/types": "dev-main",
    "northrook/support": "dev-main",
    "symfony/filesystem": "^7",
    "northrook/symfony-core-bundle": "dev-main",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  }
}